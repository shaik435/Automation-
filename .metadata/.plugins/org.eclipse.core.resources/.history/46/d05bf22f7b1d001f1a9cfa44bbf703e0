package day2;

public class Switchcase2 {

	public static void main(String[] args) {
		String  day  = "Monday";
		
		switch(day)
		{
		case "Sunday" : System.out.println(1);
		          break;
		          
		case "Monday" : System.out.println(2);
                  break;
             
		case "tuesday" : System.out.println(2);
                  break;
                  
		default: System.out.println(10);
	}

	}
}
